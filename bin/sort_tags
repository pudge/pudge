#!/usr/bin/perl -s
use warnings;
use strict;

our $r;

our @list = sortfoo([<>]);
@list = reverse @list if $r;

print join "\n", @list, "";

sub sortfoo {
    my($list) = @_;
    chomp @$list;
    
    map { $_->[0] }
    sort {
        my $arr = $a->[1];
        my $brr = $b->[1];
        my $cmp = 0;
        for my $i (0 .. $#{$arr}) {
            my $aa = $arr->[$i] // '';
            my $bb = $brr->[$i] // '';
            if (!length($aa) || !length($bb) || $aa =~ /\D/ || $bb =~ /\D/) {
                $cmp = $aa cmp $bb;
                last if $cmp;
            }
            else {
                $cmp = $aa <=> $bb;
                last if $cmp;
            }
        }
        return $cmp;
    }
    map {
        [ $_, [ (my @a = split /[_.-]/), (0)x10 ] ]
    } @$list;
}
